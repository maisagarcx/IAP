programa
{
	funcao inicio()
	{
		//variaveis
		real vetor[3]
		inteiro i

		//entrada de valores
		para(i = 0; i < 3; i++)
		{
			leia(vetor[i])
		}

		//saida de valores
		para(i = 0; i < 3; i++)
		{
			escreva(vetor[i])
		}
	}
}
